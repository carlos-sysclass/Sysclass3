            </div>
            <div class="modal-footer">
                <button type="button" class="btn default" data-dismiss="modal">{translateToken value="Close"}</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
<!-- /.modal-dialog -->
</div>
